

      {
        "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
        "width": 1000,
        "height": 600,
        "title": "Average temperature in Australia",
        "projection": {"type": "mercator", "scale": 799, "rotate": [-135,26,0]},
        "layer": [
        {
        "data": {
        "url":
        "https://raw.githubusercontent.com/Alan-Nguyen0/Data-Visualisation-2/main/map.geojson",
              "format": {"type": "topojson", "feature": "states"}
        },
         
        "mark": {"type": "geoshape", "fill": "skyblue", "stroke":
        "darkgrey"}
        },
        {
            "data": {
            "url":
            "https://raw.githubusercontent.com/Alan-Nguyen0/Data-Visualisation-2/main/weatherAUS-%20Single%20Day.csv"
            ,"format": {"type": "csv", "parse": {"Date" : "date", "longitude" : "number", "latitude" : "number"}}},
            
            "mark": {"type": "circle"},
            
            
            "encoding": {
            "longitude": {"field": "longitude", "type":
            "quantitative"},
            "latitude": {"field": "latitude", "type":
            "quantitative"},
            "size": {
            "field": "maxTemp",
            "type": "quantitative",
            "title": "Average temperature",
            "scale": {"domain": [20, 40]}
            },
            "color": {
            "field": "minTemp",
            "type": "quantitative",
            "title": "minTemp",
            "scale": {"scheme": "reds"}
            }
            }
            }
            ]
            }
            